{
  "AllowedOrigins": [
    "http://localhost:3000"    
  ],
  "ConnectionStrings": {
    "Database": ""
  },
  "SecretsApi": {
    "MailAddress": "",
    "MailPassword": "",
    "WebUrl": "",
    "Environment": ""
  },
  "AWS": {
    "Profile": "",
    "Region": "",
    "AccessKey": "",
    "SecretKey": "",
    "BucketName": ""
  },
  "Serilog": {
    "Using": [
      "Serilog.Sinks.Console",
      "Serilog.Sinks.Seq"
    ],
    "MinimumLevel": {
      "Default": "Information",
      "Override": {
        "Microsoft": "Information"
      }
    },
    "WriteTo": [
      { "Name": "Console" },
      {
        "Name": "Seq",
        "Args": { "ServerUrl": "http://seq:5341" }
      }
    ],
    "Enrich": [ "FromLogContext", "WithMachineName", "WithThreadId" ]
  },
  "Jwt": {
    "Secret": "super-duper-secret-value-that-should-be-in-user-secrets",
    "Issuer": "clean-architecture",
    "Audience": "developers",
    "ExpirationInMinutes": 15
  }
}
